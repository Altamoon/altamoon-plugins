{
  "compilerOptions": {
    "module": "es6",
    "target": "ES2021",
    "lib": ["dom", "ES2021"],
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "strict": true
  }
}